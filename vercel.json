{
  "version": 2,
  "builds": [
    {
      "src": "src/server.ts", 
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "(.*)",
      "dest": "src/server.ts"
    }
  ],
  "functions": {
    "api/reports": {
      "runtime": "nodejs16.x",
      "environment": {
        "PUPPETEER_EXECUTABLE_PATH": "/usr/bin/google-chrome"
      }
    }
  }
}